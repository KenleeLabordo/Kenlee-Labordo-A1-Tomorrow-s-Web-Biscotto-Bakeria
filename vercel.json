{
  "version": 2,
  "buildCommand": "cd biscotto-bakeria && npm install && npm run build",
  "outputDirectory": "biscotto-bakeria/dist",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
